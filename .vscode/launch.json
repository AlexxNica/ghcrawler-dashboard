      "runtimeExecutable": null,
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "localhost",
        "CONFIGURATION_ENVIRONMENT": "localhost",
        "PORT": "4000",
        "DEBUG": "appinsights",
